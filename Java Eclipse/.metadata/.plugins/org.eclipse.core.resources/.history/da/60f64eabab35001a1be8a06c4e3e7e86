package J1SP0065;

public class Student {
    private String name;
    private String Class;
    private double Math;
    private double Physyal;
    private double Chemistry;
    private double Average;
    private String type;
    public Student(String name, String class1, double math, double physyal, double chemistry,
                    double average, String type) {
	super();
	this.name = name;
	Class = class1;
	Math = math;
	Physyal = physyal;
	Chemistry = chemistry;
	Average = average;
	this.type = type;
    }
    
    
}
