package MyLinkedList;

public class Node {
    String data;
    Node ptr;

    public Node(String data) {
	super();
	this.data = data;
	this.ptr = null;
    }

    public Node() {
	this.ptr = null;
    }

    public String getData() {
	return data;
    }

    public void setData(String data) {
	this.data = data;
    }

    public Node getPtr() {
	return ptr;
    }

    public void setPtr(Node ptr) {
	this.ptr = ptr;
    }

}